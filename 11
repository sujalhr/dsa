import numpy as np
import pandas as pd

csvframe = pd.read_csv('m1.csv')  # pandas I/O API to read csv file
print(csvframe)

frame2 = pd.DataFrame(np.arange(12).reshape((4,3)),
                      index=['blue','green','white','yellow'],
                      columns=['mug','pen','ball'])
print(frame2)

# pandas I/O API to write DataFrame to csv format
frame2.to_csv('my2.csv')

# to check the content, in the current directory double click on the file 'my2.csv'
print(pd.read_csv('m1.csv', skiprows=[2], nrows=3, header=None))
