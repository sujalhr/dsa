import numpy as np
import pandas as pd
frame1 = pd.DataFrame(np.arange(16).reshape((4,4)),
index=['red','blue','yellow','white'], columns=['ball','pen','pencil','paper'])
print(frame1)
print('\n---------------converting Data frame to HTML tags-------------\n')
print(frame1.to_html())
(frame1.to_html())
print('\n---------------converting HTML tags of Data frame to DataFrame
structure------------\n')
pd.read_html('m3.html')
